{
    "name": "kunstmaan/node-search-bundle",
    "type": "symfony-bundle",
    "description": "This bundle uses the KunstmaanSearchBundle to search through Nodes from the KunstmaanNodeBundle",
    "keywords": ["kunstmaan", "node", "search", "elasticsearch"],
    "homepage": "https://github.com/Kunstmaan/KunstmaanNodeSearchBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kunstmaan Team",
            "homepage": "http://www.kunstmaan.be/",
            "email": "support@kunstmaan.be"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "~2.4",
        "sherlock/sherlock": "0.1.*",
        "kunstmaan/node-bundle": "2.4.*",
        "kunstmaan/pagepart-bundle": "2.4.*",
        "kunstmaan/search-bundle": "2.4.*",
        "kunstmaan/utilities-bundle": "2.4.*"
    },
    "require-dev": {
        "symfony-cmf/routing": "~1.1.0-dev",
        "symfony-cmf/routing-bundle": "1.1.x-dev",
        "knplabs/knp-menu": "2.0.0-alpha1@dev",
        "knplabs/knp-menu-bundle": "2.0.0-alpha1@dev",
        "pagerfanta/pagerfanta": "1.0.*@dev",
        "white-october/pagerfanta-bundle": "1.0.*@dev",
        "doctrine/data-fixtures": "1.0.*@dev",
        "doctrine/doctrine-fixtures-bundle": "2.1.*@dev",
        "friendsofsymfony/user-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": { "Kunstmaan\\NodeSearchBundle": "" }
    },
    "target-dir": "Kunstmaan/NodeSearchBundle"
}
